# CharForgeX Environment Variables

# Hugging Face
HF_TOKEN=your_huggingface_token_here
HF_HOME=./cache/huggingface

# CivitAI (for downloading additional models)
CIVITAI_API_KEY=your_civitai_api_key_here

# Google AI (for advanced captioning with Gemini)
GOOGLE_API_KEY=your_google_api_key_here

# fal.ai (for additional services)
FAL_KEY=your_fal_key_here

# RunPod (for cloud compute)
RUNPOD_API_KEY=your_runpod_api_key_here

# Replicate (optional)
REPLICATE_API_TOKEN=your_replicate_token_here

# OpenAI (optional, for enhanced captioning)
OPENAI_API_KEY=your_openai_key_here

# System paths
APP_PATH=.
WORK_DIR=./workspace
CACHE_DIR=./cache
OUTPUT_DIR=./output

# Compute settings
CUDA_VISIBLE_DEVICES=0
PYTORCH_CUDA_ALLOC_CONF=max_split_size_mb:512
